{
  "name": "short-form-video-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cross-env NODE_ENV=development tsx server/index.ts",
    "start:server": "cross-env NODE_ENV=production tsx server/index.ts",
    "dev:server": "cross-env NODE_ENV=development nodemon --exec tsx server/index.ts",
    "test:stripe": "NODE_ENV=development tsx server/test/stripe.test.ts",
    "dev:electron": "vite --config electron.vite.config.ts",
    "build:electron": "vue-tsc && vite build && electron-builder",
    "preview:electron": "electron-builder --dir",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "test": "jest",
    "start:api": "tsx server.ts",
    "dev:api": "nodemon --exec tsx server.ts"
  },
